




<div class="container col-md-12 text-center mt-3">
<tr>
        <th colspan="3" class="text text-primary"><h2>Category Toy</h2></th>

    <table class="table table-info">

    <thead>
        
        <tr>
            <th>Name Category</th>
            <th>Image</th>
			<th>Product</th>
            
           
        </tr>
    </thead>
    <tbody>
 
    {{#each category }}
   <tr>
   <td><a>{{name}}</a></td>
    <td>
        <a href="/category/detail/{{_id}}">
            <img src="{{image}}" width="180" height="180">
        </a>
	</td>
	<td>
		<a href="/">View Product</a>
	</td>
    
      
   </tr>
   {{/each}}
       
    </tbody>
  </table>
</div>



